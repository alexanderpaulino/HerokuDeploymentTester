  <div class="jumbotron bg-primary text-center">
      <h2>Burger Eater</h2>
  </div>
    
  <div class = "container-fluid burgersContainer" >
  <div class="jumbotron burgersJumbo" style="background-color:lightbl">
<h1>Burgers!</h1>

<h2>Add a Burger</h2>
<form class="create-form">

  <div class="form-group">
    <label for="ca">Burger Name:</label>
    <input type="text" id="ca" name="name">
  </div>


  <button type="submit">Add burger</button>
</form>
<h2>Burgers</h2>
{{#each burgers}}
{{#if this.eaten}}
{{else}}
<ul>
    <li>
     <p><span class="labels">Burger:</span> {{this.name}}</p>
    <button class="eat-burger" data-id="{{this.id}}">Eat Burger</button>
     </li>
    {{/if}}
    </ul>
  {{/each}}
<h2>Eaten Burgers!</h2>
<ul>
  {{#each burgers}}
    {{#if this.eaten}}
  <li class = "eaten">
      <p><span class="labels">Burger:</span> {{this.name}}</p>

  	</li>
    {{/if}}
  {{/each}}
  </ul>
</div>
</div>

